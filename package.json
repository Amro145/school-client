{
  "name": "school-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo --port 8787",
    "build": "next build",
    "dev:cloudflare": "wrangler pages dev --port 8787 --compatibility-flag=nodejs_compat -- npx next dev",
    "pages:build": "npx @cloudflare/next-on-pages",
    "predev:cloudflare": "npm run clean:port",
    "preview": "npm run build:cloudflare && wrangler pages dev .vercel/output/static --port 8787",
    "prepreview": "npm run clean:port",
    "clean:port": "fuser -k 8787/tcp || true"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "axios": "^1.13.2",
    "drizzle-orm": "^0.45.1",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.562.0",
    "next": "^15.5.9",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@cloudflare/workers-types": "^4.20251221.0",
    "@eslint/eslintrc": "^3.3.3",
    "@opennextjs/cloudflare": "^1.14.7",
    "@tailwindcss/postcss": "^4.0.0-alpha.33",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4.0.0-alpha.33",
    "typescript": "^5",
    "wrangler": "^4.56.0"
  }
}